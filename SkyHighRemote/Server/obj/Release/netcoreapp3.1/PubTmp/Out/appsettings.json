{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "SkyHighRemoteLog.txt",
          "rollingInterval": "Day",
          "retainedFileCount": 7
        }
      }
    ]
  },
  "Version": {
    "Server": "1.0"
  }
}
